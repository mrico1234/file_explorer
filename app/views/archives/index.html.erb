<p id="notice"><%= notice %></p>

<h1>Archivos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Creationdate</th>
      <th>Location</th>
      <th>Tamaño</th>
      <!--<th>String</th>-->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @archives.each do |archive| %>
      <tr>
        <td><%= archive.name %></td>
        <td><%= archive.typ %></td>
        <td><%= archive.creationdate %></td>
        <td><%= archive.location %></td>
        <td><%= archive.tamaño %></td>
        <!--<td><%= archive.string %></td>-->
        <td><%#= link_to 'Show', archive %></td>
        <td><%#= link_to 'Edit', edit_archive_path(archive) %></td>
        <td><%#= link_to 'Destroy', archive, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%#= link_to 'Nuevo archivo', new_archive_path %>
